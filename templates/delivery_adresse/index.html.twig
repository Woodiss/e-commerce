{% extends 'base.html.twig' %}

{% block title %}DeliveryAdresse index{% endblock %}

{% block body %}
    <h1>DeliveryAdresse index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>City</th>
                <th>Postal_code</th>
                <th>Full_name</th>
                <th>Phone_number</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for delivery_adresse in delivery_adresses %}
            <tr>
                <td>{{ delivery_adresse.id }}</td>
                <td>{{ delivery_adresse.City }}</td>
                <td>{{ delivery_adresse.postalCode }}</td>
                <td>{{ delivery_adresse.fullName }}</td>
                <td>{{ delivery_adresse.phoneNumber }}</td>
                <td>
                    <a href="{{ path('app_delivery_adresse_show', {'id': delivery_adresse.id}) }}">show</a>
                    <a href="{{ path('app_delivery_adresse_edit', {'id': delivery_adresse.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_delivery_adresse_new') }}">Create new</a>
{% endblock %}
